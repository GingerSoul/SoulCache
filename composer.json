{
    "name": "gingersoul/soul-precache",
    "description": "A WP plugin for pre-caching assets in the browser.",
    "license": "GPLv3",
    "prefer-stable": true,
    "type": "wordpress-plugin",
    "authors": [
        {
            "name": "Anton Ukhanev",
            "email": "xedin.unknown@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^5.4 | ^7.0",
        "psr/container": "^1.0",
        "dhii/di": "^0.2-alpha1",
        "dhii/memoize-memory": "^0.2-alpha1"
    },
    "require-dev": {
        "wp-coding-standards/wpcs": "^1.0",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.4.3",
        "wpackagist-plugin/meta-box": "^4.14"
    },
    "autoload": {
        "classmap": ["classes/"]
    },
    "repositories":[
        {
            "type":"composer",
            "url":"https://wpackagist.org"
        }
    ],
    "scripts": {
        "cscheck": "phpcs -s -n",
        "csfix": "phpcbf --standard=phpcs.xml"
    },
    "extra": {
        "installer-paths": {
            "../{$name}/": ["type:wordpress-plugin"]
        }
    }
}
